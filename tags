!_TAG_FILE_FORMAT	2	/extended format; --format=1 will not append ;" to lines/
!_TAG_FILE_SORTED	1	/0=unsorted, 1=sorted, 2=foldcase/
!_TAG_PROGRAM_AUTHOR	Darren Hiebert	/dhiebert@users.sourceforge.net/
!_TAG_PROGRAM_NAME	Exuberant Ctags	//
!_TAG_PROGRAM_URL	http://ctags.sourceforge.net	/official site/
!_TAG_PROGRAM_VERSION	5.8	//
Bdelete	plugin/bbye.vim	/^command! -bang -complete=buffer -nargs=? Bdelete$/;"	c
Bdeleteall	plugin/bbye.vim	/^command! -bang -complete=buffer -nargs=? Bdeleteall$/;"	c
Bdeleteexcept	plugin/bbye.vim	/^command! -bang -complete=buffer -nargs=? Bdeleteexcept$/;"	c
VERSION	Makefile	/^VERSION := 1.0.1$/;"	m
bdelete	plugin/bbye.vim	/^function! s:bdelete(bang, buffer_name)$/;"	f
bdeleteall	plugin/bbye.vim	/^function! s:bdeleteall(bang)$/;"	f
bdeleteexcept	plugin/bbye.vim	/^function! s:bdeleteexcept(bang, buffer_name)$/;"	f
g:loaded_bbye	plugin/bbye.vim	/^let g:loaded_bbye = 1$/;"	v
new	plugin/bbye.vim	/^function! s:new(bang)$/;"	f
str2bufnr	plugin/bbye.vim	/^function! s:str2bufnr(buffer)$/;"	f
warn	plugin/bbye.vim	/^function! s:warn(msg)$/;"	f
